    "size": [  59, 9 ]
  },
  "arena_text_defeat@arena_image_template": {
    "$arena_text_texture": "announcer_texts",
    "uv": [ 0, 54 ],
    "uv_size": [  107, 18 ],
    "size": [  53.5, 9 ]
  },
  "arena_text_3@arena_image_template": {
    "$arena_text_texture": "announcer_texts",
    "uv": [ 139, 36 ],
    "uv_size": [  72, 18 ],
    "size": [  72, 18 ]
  },
  "arena_text_2@arena_image_template": {
