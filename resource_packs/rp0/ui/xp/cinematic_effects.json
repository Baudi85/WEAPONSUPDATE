  "arena_lives_uv_b3@xp_podium_screen.weapon_stats_uv_base": { "$set_fps": 0.1, "$set_image_uv": [ 0, 45 ], "$set_image_uv_next": "@xp_cinematic_effects.arena_lives_uv_c3" },
